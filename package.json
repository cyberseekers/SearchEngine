      "plugin:@typescript-eslint/recommended",
      "next/core-web-vitals",
      "plugin:unicorn/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "unicorn/prefer-module": "off",
      "@typescript-eslint/no-var-requires": "off"
    },
    "root": true
  }
}
