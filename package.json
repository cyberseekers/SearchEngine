{
  "name": "test",
  "version": "0.1.0",
  "homepage": "https://hoangtran12.github.io/test/",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@reduxjs/toolkit": "^2.2.5",
    "gh-pages": "^6.1.1",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.9",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "postcss": "^8",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "eslintConfig": {
    "extends": [
